{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:react/recommended"
  ],
  "plugins": ["react", "prettier"],
  "env": { 
    "browser": true,
    "es6": true,
    "jquery": true
    
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "max-len": ["error", {"code": 100}],
    "react/prop-types": ["warn"],
    "no-return-assign": ["off"]
  }
}